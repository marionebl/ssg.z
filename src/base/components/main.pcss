@import './theme.pcss';

.image-wrapper {
  position: relative;
  border: 20px solid grey(50);

  .image-preview .image-main {
    position: absolute;
    width: 100%;
    z-index: 1;
    top: 0;
    left: 0;
  }

  .image-main {
    display: none;

    &.image-loaded {
      display: inline;
    }
  }
}
